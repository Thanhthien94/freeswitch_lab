<profile name="external">
  <gateways>
    <X-PRE-PROCESS cmd="include" data="external/*.xml"/>
  </gateways>

  <domains>
    <domain name="all" alias="true" parse="false"/>
  </domains>

  <settings>
    <param name="debug" value="0"/>
    <param name="sip-trace" value="false"/>
    <param name="odbc-dsn" value=""/>
    <param name="sip-port" value="$${external_sip_port}"/>
    <param name="rtp-ip" value="$${external_rtp_ip}"/>
    <param name="sip-ip" value="$${external_sip_ip}"/>
    <param name="ext-sip-ip" value="$${external_sip_ip}"/>
    <param name="ext-rtp-ip" value="$${external_rtp_ip}"/>
    <param name="sip-tcp-port" value="0"/>
    <param name="sip-tls-port" value="0"/>
    <param name="dtmf-duration" value="2000"/>
    <param name="rtp-timeout-sec" value="300"/>
    <param name="rtp-hold-timeout-sec" value="1800"/>
    <param name="hold-music" value="local_stream://moh"/>
    <param name="manual-redirect" value="true"/>
    <param name="context" value="public"/>
    <param name="caller-id-type" value="rpid"/>
    <param name="nonce-ttl" value="60"/>
    <param name="auth-calls" value="false"/>
    <param name="inbound-codec-negotiation" value="generous"/>
    <param name="inbound-late-negotiation" value="true"/>
    <param name="inbound-proxy-media" value="false"/>
    <param name="outbound-proxy-media" value="false"/>
    <param name="inbound-codec-prefs" value="$${global_codec_prefs}"/>
    <param name="outbound-codec-prefs" value="$${outbound_codec_prefs}"/>
    <param name="media-option" value="resume-media-on-hold"/>
    <param name="disable-transcoding" value="false"/>
    <param name="fs-path" value="/fs"/>
  </settings>
</profile>
