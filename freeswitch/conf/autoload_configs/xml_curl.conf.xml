<X-PRE-PROCESS cmd="include" data="../vars_local.xml"/>
<configuration name="xml_curl.conf" description="XML Fetch via HTTP">
  <bindings>
    <binding name="default">
      <param name="gateway-url" value="$${xml_curl_gateway_url}"/>
      <param name="gateway-method" value="GET"/>
      <param name="gateway-timeout" value="2000"/>
      <param name="gateway-cache" value="false"/>
      <param name="gateway-retries" value="2"/>
      <param name="scope" value="directory,dialplan"/>
    </binding>
  </bindings>
</configuration>
