<configuration name="xml_curl.conf" description="XML Fetch via HTTP">
  <settings>
    <!-- We use simple GET to our NestJS service -->
    <param name="url" value="http://app:3000/fs/xml"/>
    <param name="cacheable" value="false"/>
    <param name="method" value="GET"/>
    <param name="timeout" value="2000"/>
  </settings>
</configuration>
