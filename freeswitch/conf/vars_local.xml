<include>
  <X-PRE-PROCESS cmd="set" data="auto_nat=true"/>
  <X-PRE-PROCESS cmd="set" data="sip_tls_version=tlsv1_2"/>
  <X-PRE-PROCESS cmd="set" data="external_sip_ip=auto-nat"/>
  <X-PRE-PROCESS cmd="set" data="external_rtp_ip=auto-nat"/>
  <X-PRE-PROCESS cmd="set" data="xml_cdr_url=http://127.0.0.1:3000/fs/cdr"/>
  <X-PRE-PROCESS cmd="env-set" data="xml_cdr_url=$XML_CDR_URL"/>
  <X-PRE-PROCESS cmd="set" data="xml_curl_gateway_url=http://localhost:3000/fs/xml"/>
  <X-PRE-PROCESS cmd="env-set" data="xml_curl_gateway_url=$XML_CURL_GATEWAY_URL"/>
  <X-PRE-PROCESS cmd="set" data="internal_sip_port=5060"/>
  <X-PRE-PROCESS cmd="set" data="internal_tls_port=5061"/>
  <X-PRE-PROCESS cmd="set" data="external_sip_port=5080"/>
  <X-PRE-PROCESS cmd="set" data="external_tls_port=5081"/>
  <X-PRE-PROCESS cmd="set" data="rtp_start_port=16384"/>
  <X-PRE-PROCESS cmd="set" data="rtp_end_port=16420"/>
  <X-PRE-PROCESS cmd="set" data="event_socket_port=8021"/>
  <X-PRE-PROCESS cmd="set" data="internal_ws_port=5066"/>
  <X-PRE-PROCESS cmd="set" data="internal_wss_port=7443"/>
  <X-PRE-PROCESS cmd="include" data="vars_local.d/*.xml"/>
</include>
